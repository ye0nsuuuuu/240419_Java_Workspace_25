package ex_240419;

public class Hello {

	public static void main(String[] args) {
		// 규칙1, 메인 함수(메서드)에서 시작하고, 끝난다.
		// 규칙2, 파일명, 클래스명이 동일 해야한다. 
		// 글자 폰트 크기 단축키 : ctrl + shift + "+" 또는 "-"
		// 코딩창 만 보기 : ctrl + m 
		// 실행 디버깅 없이 : ctrl + f11 
		System.out.println("Hello Java World");
		// 클래스 파일 만들고, 오늘 먹을 점심 메뉴 출력해보기.
		
		// 자동 완성. 
		

	}

}
